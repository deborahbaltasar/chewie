{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "airbnb",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
    
      "sourceType": "module"
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "global-require": "off",
      "import/prefer-default-export": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePatter": "^_"
        }
      ]
    }
  }